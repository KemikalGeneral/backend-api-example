	"description": "Example Express + TypeScript API",
	"main": "dist/server.js",
	"scripts": {
		"build": "tsc",
		"dev": "nodemon --exec ts-node src/server.ts",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
