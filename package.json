		"@eslint/eslintrc": "^3.3.3",
		"@eslint/js": "^9.39.2",
		"@types/express": "^5.0.6",
		"@types/node": "^25.0.10",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-unused-imports": "^4.4.1",
		"globals": "^17.3.0",
		"nodemon": "^3.1.11",
		"prettier": "^3.8.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0"
	}
